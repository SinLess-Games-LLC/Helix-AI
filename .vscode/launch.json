{
  "version": "0.1.0",
  "configurations": [
    {
    "name": "Run Extension In Dev Mode",
    "type": "extensionHost",
    "request": "launch",
    "runtimeExecutable": "${execPath}",
    "args": [
      "--extensionDevelopmentPath=/dist/apps/DevSecOps-Dashboard"
    ],
    "trace": false,
    "internalConsoleOptions": "openOnFirstSessionStart",
    "outFiles": [
      "${workspaceFolder}/dist/apps/DevSecOps-Dashboard"
    ],
    "preLaunchTask": "${defaultBuildTask}"
  }]
}
