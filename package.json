{
  "name": "@helix-ai/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:auth-server": "nx run auth-server:build:development",
    "build:frontend": "nx run frontend:build:development",
    "docker-compose:down": "docker-compose -f ./docker/docker-compose.yaml down",
    "docker-compose:restart": "npm run docker-compose:down && npm run docker-compose:up",
    "docker-compose:up": "docker-compose -f ./docker/docker-compose.yaml up -d",
    "prepare": "husky",
    "serve": "npm run serve:frontend:dev && npm run serve:auth-server",
    "serve:auth-server": "nx run auth-server:serve",
    "serve:frontend:dev": "nx run frontend:dev --port 3001",
    "setup": "npm run setup:npm && npm run setup:python && npm run setup:docker",
    "setup:docker": "docker-compose -f ./docker/docker-compose.yaml up -d",
    "setup:npm": "npm install",
    "setup:python": "nx run @nxlv/python:install",
    "teardown": "npm run teardown:npm && npm run teardown:python && npm run teardown:docker",
    "teardown:docker": "docker-compose -f ./docker/docker-compose.yaml down",
    "teardown:npm": "rm -rf node_modules",
    "teardown:python": "rm -rf ./apps/ai/helix/.venv",
    "test": "nx affected:test",
    "update": "npm update"
  },
